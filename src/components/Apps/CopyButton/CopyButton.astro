---
import "./button.css";
---


  <button id="copyBtn" class="copyButton" onclick="navigator.clipboard.writeText(getElementByTagName('CopyButton').innerHTML)">
    <div class="contents">
      <i class="far fa-clipboard"></i>
      <div>
        <h1 class="topText">Import Code.</h1>
        <h2 class="bottomText">Click to Copy.</h2>
      </div>
    </div>
  </button>
