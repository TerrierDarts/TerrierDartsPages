{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "jsxImportSource": "preact",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "strict": true,
    "esModuleInterop": true,
    "types": ["astro"]
  }
}